Subject: [PATCH 21/99] contribs: gcrypt: Disable asm when building with
 visualstudio

---
 contrib/src/gcrypt/rules.mak | 3 +++
 1 file changed, 3 insertions(+)

diff --git a/contrib/src/gcrypt/rules.mak b/contrib/src/gcrypt/rules.mak
index 4b2cce0..6ba2eaa 100644
--- a/contrib/src/gcrypt/rules.mak
+++ b/contrib/src/gcrypt/rules.mak
@@ -24,6 +24,9 @@ GCRYPT_CONF = \
 ifdef HAVE_WIN64
 GCRYPT_CONF += --disable-asm
 endif
+ifdef HAVE_VISUALSTUDIO
+GCRYPT_CONF += --disable-asm
+endif
 ifdef HAVE_IOS
 GCRYPT_EXTRA_CFLAGS = -fheinous-gnu-extensions
 else
