From 5000a09320722cd66a717d9cd42edc36923bb84b Mon Sep 17 00:00:00 2001
From: =?UTF-8?q?Hugo=20Beauz=C3=A9e-Luyssen?= <hugo@beauzee.fr>
Date: Mon, 5 Jan 2015 14:13:38 +0100
Subject: [PATCH 61/62] FIXUP contrib HAVE_WINDOWSRT

---
 contrib/src/ffmpeg/rules.mak | 2 +-
 contrib/src/iconv/rules.mak  | 2 +-
 contrib/src/png/rules.mak    | 2 +-
 3 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/contrib/src/ffmpeg/rules.mak b/contrib/src/ffmpeg/rules.mak
index 0b44756..c4922fd 100644
--- a/contrib/src/ffmpeg/rules.mak
+++ b/contrib/src/ffmpeg/rules.mak
@@ -142,7 +142,7 @@ FFMPEGCONF += --enable-w32threads --enable-dxva2 \
 ifdef HAVE_WIN64
 FFMPEGCONF += --cpu=athlon64 --arch=x86_64
 else # !WIN64
-ifdef HAVE_WINPHONE
+ifdef HAVE_WINDOWSRT
 FFMPEGCONF += --arch=arm --cpu=armv7-a --target-os=win32 --extra-cflags='-DWINAPI_FAMILY=WINAPI_FAMILY_APP -MD -D__ARM_PCS_VFP -D_WIN32_WINNT=0x602' --disable-dxva2 --as=armasm
 else
 FFMPEGCONF+= --cpu=i686 --arch=x86
diff --git a/contrib/src/iconv/rules.mak b/contrib/src/iconv/rules.mak
index 11728e1..ae236ea 100644
--- a/contrib/src/iconv/rules.mak
+++ b/contrib/src/iconv/rules.mak
@@ -27,7 +27,7 @@ endif
 ifdef HAVE_IOS
 	$(APPLY) $(SRC)/iconv/libiconv-android-ios.patch
 endif
-ifdef HAVE_WINPHONE
+ifdef HAVE_WINDOWSRT
 	$(APPLY) $(SRC)/iconv/libiconv-android-ios.patch
 endif
 ifdef HAVE_WINRT
diff --git a/contrib/src/png/rules.mak b/contrib/src/png/rules.mak
index c947f1b..31ca4df 100644
--- a/contrib/src/png/rules.mak
+++ b/contrib/src/png/rules.mak
@@ -21,7 +21,7 @@ png: libpng-$(PNG_VERSION).tar.xz .sum-png
 
 DEPS_png = zlib $(DEPS_zlib)
 
-ifdef HAVE_WINPHONE
+ifdef HAVE_WINDOWSRT
 CONF = --disable-arm-neon
 endif
 
-- 
2.2.0

