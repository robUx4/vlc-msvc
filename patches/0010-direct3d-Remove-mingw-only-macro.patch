     }
 
     hr = IDirect3DDevice9_CreatePixelShader(sys->d3ddev,
-            ID3DXBuffer_GetBufferPointer(compiled_shader),
+            compiled_shader->lpVtbl->GetBufferPointer(compiled_shader),
             &sys->d3dx_shader);
 
     if (FAILED(hr)) {
