Date: Thu, 20 Nov 2014 16:40:17 +0100
Subject: [PATCH 52/74] contribs: speedxdsp: Disable ASM on windows phone

---
 contrib/src/speexdsp/rules.mak | 4 ++++
 1 file changed, 4 insertions(+)

diff --git a/contrib/src/speexdsp/rules.mak b/contrib/src/speexdsp/rules.mak
index 3cc2b15..fe98edf 100644
--- a/contrib/src/speexdsp/rules.mak
+++ b/contrib/src/speexdsp/rules.mak
@@ -37,6 +37,10 @@ SPEEXDSP_CONF += --enable-arm5e-asm
 endif
 endif
 
+ifdef HAVE_WINPHONE
+SPEEXDSP_CONF += --disable-neon
+endif
+
 .speexdsp: speexdsp
 	mkdir -p $</m4 && $(RECONF)
 	cd $< && $(HOSTVARS) ./configure $(HOSTCONF) $(SPEEXDSP_CONF)
