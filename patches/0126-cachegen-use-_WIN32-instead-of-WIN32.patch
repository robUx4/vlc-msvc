---
 bin/cachegen.c | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/bin/cachegen.c b/bin/cachegen.c
index 39b3f1d..397d402 100644
--- a/bin/cachegen.c
+++ b/bin/cachegen.c
@@ -29,7 +29,7 @@
 #ifdef HAVE_GETOPT_H
 # include <getopt.h>
 #endif
-#ifdef WIN32
