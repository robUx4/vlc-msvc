 # include <windows.h>
 #endif
 
@@ -48,7 +48,7 @@ static void usage (const char *path)
 
 int main (int argc, char *argv[])
 {
-#ifdef WIN32
+#ifdef _WIN32
     SetErrorMode(SEM_FAILCRITICALERRORS);
 #endif
 #ifdef HAVE_GETOPT_H
